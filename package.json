{
  "name": "@fitness-foods/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev --transpile-only -r tsconfig-paths/register -r dotenv/config ./src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watchAll",
    "test:coverage": "jest --coverage",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:dev": "dotenv sequelize db:migrate",
    "db:migrate:undo:dev": "dotenv sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:seed:dev": "dotenv sequelize db:seed:all",
    "db:drop:dev": "dotenv sequelize db:drop",
    "db:create:dev": "dotenv sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:seed:undo:dev": "dotenv sequelize db:seed:undo:all"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.1.6",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "jsdom": "^24.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.7",
    "sequelize": "^6.21.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@faker-js/faker": "^7.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/nodemailer": "^6.4.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^7.4.2",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
